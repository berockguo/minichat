include ../../comm.mk

include ../../phxrpc/phxrpc.mk

CFLAGS += $(REDIS_CLIENT_INCLUDE)

LDFLAGS := -L$(PHXRPC_ROOT)/lib -lphxrpc $(LDFLAGS) $(REDIS_CLIENT_LIBRARY)

LIBCOMMON_OBJS = redis_utils.o

TARGETS = libcommon.a

all: $(TARGETS)

libcommon.a: $(LIBCOMMON_OBJS)
	$(AR) $@ $^
	mkdir -p $(MINICHAT_ROOT)/lib; cp $@ $(MINICHAT_ROOT)/lib;

clean:
	@($(RM) $(TARGETS))
	@($(RM) *.o)
	@($(RM) phxrpc_*)

